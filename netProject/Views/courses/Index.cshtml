@model IEnumerable<netProject.Models.course>

@{
    ViewBag.Title = "Courses";
    
Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.day)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.st_hour)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.end_hour)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.classroom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Moad1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Moad2)
        </th>
        <th></th>
     
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.day)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.st_hour)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.end_hour)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.classroom)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Moad1)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Moad2)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
